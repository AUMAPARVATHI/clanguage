#include <stdio.h>

int main()
{
 int i,arr[100] ,n,count=0;
 printf("Enter the size of an array  :");
 scanf("%d",&n);
 for (i = 0; i < n; i++)
 scanf("%d",&arr[i]);
 for(i=0;i<n;i++)
 {
  if(arr[i]!=0)
  arr[count++]=arr[i];
  }
 for(i=count;i<n;i++)
 {
 arr[i]=0;
 }
 for(i=0;i<n;i++)
 printf("%d\t",arr[i]);
return 0;
}

   
